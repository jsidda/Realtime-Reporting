	<subDataset name="Adjustment_Dataset" uuid="1654fbc1-876e-4efd-8d3c-8482e0db7e7b">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="EBIZ-silo5"/>
		<parameter name="Invoice_Start_Date" class="java.sql.Date"/>
		<parameter name="Invoice_End_Date" class="java.sql.Date"/>
		<parameter name="Location" class="java.util.Collection"/>
		<parameter name="Adjustment_Start_Date" class="java.sql.Date" isForPrompting="false">
			<parameterDescription><![CDATA[]]></parameterDescription>
		</parameter>
		<parameter name="Adjustment_End_Date" class="java.sql.Date" isForPrompting="false"/>
		<parameter name="Invoice_Start_Date_sql" class="java.lang.String">
			<defaultValueExpression><![CDATA[$P{Invoice_Start_Date}!=null? "and trunc(invoice_date) >= to_date('"+$P{Invoice_Start_Date}+"', 'YYYY-MM-DD')":""]]></defaultValueExpression>
		</parameter>
		<parameter name="Invoice_End_Date_sql" class="java.lang.String">
			<defaultValueExpression><![CDATA[$P{Invoice_End_Date}!=null?"and trunc(invoice_date) <= to_date('"+$P{Invoice_End_Date}+"', 'YYYY-MM-DD')":""]]></defaultValueExpression>
		</parameter>
		<parameter name="Invoice_Type" class="java.util.Collection"/>
		<parameter name="Bill_To_Account_Number_MSQ" class="java.util.Collection"/>
		<parameter name="Sold_To_Account_Number_MSQ" class="java.util.Collection"/>
		<parameter name="Bill_To_Group_Code_MSQ" class="java.util.Collection" isForPrompting="false"/>
		<parameter name="Sold_To_Group_Code_MSQ" class="java.util.Collection"/>
		<parameter name="Visible_Account" class="java.lang.String" isForPrompting="false">
			<defaultValueExpression><![CDATA["both"]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select *
 from
 (
  Select distinct 
rre.RESOURCE_NAME as location

, cas.account_name as sold_to_acc_name
, cas.ACCOUNT_NUMBER as sold_to_acc_num
, cab.account_name as bill_to_acc_name
, cab.ACCOUNT_NUMBER as bill_to_acc_num

, cta.TRX_NUMBER as invoice_number
, cta.attribute4 as vin
, psa.due_date as invoice_date
, decode(ctt.type, 'CM','Credit Memo', 'DM', 'Debit Memo', 'INV', 'Invoice') as Invoice_type
, rta.type as Adjustment_Type
, aaa.type
, aaa.apply_date as Adjustment_Date
,  COALESCE(rta.DESCRIPTION, rta.name) as description
-- cr1099
, rta.name as item
, aaa.reason_code
, psa.AMOUNT_DUE_ORIGINAL
, aaa.amount
, decode(aaa.status, 'A', 'Approved', 'U', 'Unaccrued', 'R', 'Rejected') as Status
, f.user_name
, COALESCE(bs.name,'Auto Invoice') as source
, cc.segment1||'.'||cc.segment2||'.'||cc.segment3||'.'||cc.segment4||'.'||cc.segment5||'.'||cc.segment6||'.'||cc.segment7 as accounting_string
from AR.HZ_CUST_ACCOUNTS cas
--bill to sold to
join  AR.RA_CUSTOMER_TRX_ALL cta on cta.SOLD_TO_CUSTOMER_ID = cas.CUST_ACCOUNT_ID
join AR.HZ_CUST_ACCOUNTS cab on cta.BILL_TO_CUSTOMER_ID = cab.CUST_ACCOUNT_ID
--join AR.RA_CUSTOMER_TRX_LINES_ALL ctla on cta.CUSTOMER_TRX_ID = ctla.CUSTOMER_TRX_ID
join AR.AR_PAYMENT_SCHEDULES_ALL psa on cta.CUSTOMER_TRX_ID = psa.CUSTOMER_TRX_ID
join ar.ar_adjustments_all aaa on aaa.CUSTOMER_TRX_ID = cta.CUSTOMER_TRX_ID
join APPLSYS.FND_USER f on aaa.created_by = f.user_id
join AR.RA_CUST_TRX_TYPES_ALL ctt on ctt.CUST_TRX_TYPE_ID = cta.CUST_TRX_TYPE_ID
join AR.AR_RECEIVABLES_TRX_ALL rta on rta.RECEIVABLES_TRX_ID = aaa.RECEIVABLES_TRX_ID
left join JTF.jtf_rs_salesreps jrs on cta.PRIMARY_SALESREP_ID = jrs.SALESREP_ID
left join JTF.JTF_RS_RESOURCE_EXTNS_tl rre on rre.RESOURCE_ID = jrs.RESOURCE_ID and rre.LANGUAGE = 'US'
left JOIN AR.ra_BATCH_SOURCES_ALL bs on cta.BATCH_SOURCE_ID = bs.BATCH_SOURCE_ID
-- Accounting String

join GL.GL_CODE_COMBINATIONS cc on rta.CODE_COMBINATION_ID = cc.CODE_COMBINATION_ID 
where 1=1


AND $X{IN,cas.attribute15,Sold_To_Group_Code_MSQ}
AND $X{IN,cab.attribute15,Bill_To_Group_Code_MSQ}
and $X{IN, ctt.CUST_TRX_TYPE_ID,Invoice_Type}

Union all

Select distinct 
rre.RESOURCE_NAME as location
, cas.account_name as sold_to_acc_num
, cas.ACCOUNT_NUMBER as sold_to_acc_name
, cab.account_name as bill_to_acc_num
, cab.ACCOUNT_NUMBER as bill_to_acc_name
, cta.TRX_NUMBER as invoice_number
, cta.attribute4 as vin
, psa.due_date as invoice_date
, decode(ctt.type, 'CM','Credit Memo', 'DM', 'Debit Memo', 'INV', 'Invoice') as Invoice_type
, 'Credit_Memo' as Adjustment_Type
, ctla.line_type  as type
, ctla.CREATION_DATE as Adjustment_Date
--cr1099
--, msi.description
, COALESCE(CTLA.TRANSLATED_DESCRIPTION, ctla.description) as description
, ctla.description as item
, ctla.REASON_CODE
, psa.AMOUNT_DUE_ORIGINAL
, (case when ctla.QUANTITY_INVOICED is null then ctla.quantity_credited else ctla.quantity_invoiced end)*ctla.UNIT_SELLING_PRICE as Amount
, 'Approved' as Status
, f.user_name
, COALESCE(os.DESCRIPTION,bs.name,'Auto Invoice') as source
, cc.segment1||'.'||cc.segment2||'.'||cc.segment3||'.'||cc.segment4||'.'||cc.segment5||'.'||cc.segment6||'.'||cc.segment7 as accounting_string
from AR.HZ_CUST_ACCOUNTS cas
--bill to sold to
join  AR.RA_CUSTOMER_TRX_ALL cta on cta.SOLD_TO_CUSTOMER_ID = cas.CUST_ACCOUNT_ID
join AR.HZ_CUST_ACCOUNTS cab on cta.BILL_TO_CUSTOMER_ID = cab.CUST_ACCOUNT_ID

join AR.RA_CUSTOMER_TRX_LINES_ALL ctla on cta.CUSTOMER_TRX_ID = ctla.CUSTOMER_TRX_ID
join AR.AR_PAYMENT_SCHEDULES_ALL psa on cta.CUSTOMER_TRX_ID = psa.CUSTOMER_TRX_ID
join APPLSYS.FND_USER f on ctla.created_by = f.user_id
join AR.RA_CUST_TRX_TYPES_ALL ctt on ctt.CUST_TRX_TYPE_ID = cta.CUST_TRX_TYPE_ID and ctt.ORG_ID = cta.ORG_ID
--join INV.MTL_SYSTEM_ITEMS_B msi on ctla.INVENTORY_ITEM_ID = msi.INVENTORY_ITEM_ID
left join JTF.jtf_rs_salesreps jrs on cta.PRIMARY_SALESREP_ID = jrs.SALESREP_ID
left join JTF.JTF_RS_RESOURCE_EXTNS_tl rre on rre.RESOURCE_ID = jrs.RESOURCE_ID and rre.LANGUAGE = 'US'
left JOIN AR.ra_BATCH_SOURCES_ALL bs on cta.BATCH_SOURCE_ID = bs.BATCH_SOURCE_ID
left join ONT.OE_ORDER_HEADERS_ALL oha on cta.ct_reference = to_char(oha.order_number)
left join ONT.OE_ORDER_LINES_ALL ola on ola.HEADER_ID = oha.HEADER_ID
left join applsys.fnd_user fo on fo.user_id = ola.CREATED_BY
left join ONT.OE_ORDER_SOURCES os on os.ORDER_SOURCE_ID=ola.ORDER_SOURCE_ID
-- Accounting String

left join ar.Ra_Cust_Trx_Line_Gl_Dist_All Trxgl on Trxgl.Customer_Trx_Line_Id=ctla.Customer_Trx_Line_Id
left join gl.GL_CODE_COMBINATIONS CC on Trxgl.Code_Combination_Id=cc.Code_Combination_Id
where   
ctt.type = 'CM'
and CTLA.SALES_ORDER is null
and ctt.name NOT IN ('US SELLER VEH AR CM','US SELLER VEH AR CM EX','US SELL VEH AR CM EX','US SELL VEH AR CM')

AND $X{IN,cas.attribute15,Sold_To_Group_Code_MSQ}
AND $X{IN,cab.attribute15,Bill_To_Group_Code_MSQ}
and $X{IN, ctt.CUST_TRX_TYPE_ID,Invoice_Type}

Union all

Select distinct 
rre.RESOURCE_NAME as location
, cas.account_name as sold_to_acc_num
, cas.ACCOUNT_NUMBER as sold_to_acc_name
, cab.account_name as bill_to_acc_num
, cab.ACCOUNT_NUMBER as bill_to_acc_name
, cta.trx_number as invoice_number
, cta.attribute4 as vin
, psa.due_date as invoice_date
, decode(ctt.type, 'CM','Credit Memo', 'DM', 'Debit Memo', 'INV', 'Invoice') as Invoice_type
, 'Debit_Memo' as Adjustment_Type
, ctla.line_type as type
, ctla.CREATION_DATE as Adjustment_Date
--cr1099
--, msi.description
, COALESCE(CTLA.TRANSLATED_DESCRIPTION, ctla.description) as description
, ctla.description as item
, ctla.REASON_CODE
, psa.AMOUNT_DUE_ORIGINAL
, (case when ctla.QUANTITY_INVOICED is null then ctla.quantity_credited else ctla.quantity_invoiced end)*ctla.UNIT_SELLING_PRICE as Amount
, 'Approved' as Status
, f.user_name as user_name

, COALESCE(bs.name,'Auto Invoice') as source
, cc.segment1||'.'||cc.segment2||'.'||cc.segment3||'.'||cc.segment4||'.'||cc.segment5||'.'||cc.segment6||'.'||cc.segment7 as accounting_string
from AR.HZ_CUST_ACCOUNTS cas
--bill to sold to
join  AR.RA_CUSTOMER_TRX_ALL cta on cta.SOLD_TO_CUSTOMER_ID = cas.CUST_ACCOUNT_ID
join AR.HZ_CUST_ACCOUNTS cab on cta.BILL_TO_CUSTOMER_ID = cab.CUST_ACCOUNT_ID

join AR.RA_CUSTOMER_TRX_LINES_ALL ctla on cta.CUSTOMER_TRX_ID = ctla.CUSTOMER_TRX_ID
join AR.AR_PAYMENT_SCHEDULES_ALL psa on cta.CUSTOMER_TRX_ID = psa.CUSTOMER_TRX_ID
join APPLSYS.FND_USER f on ctla.created_by = f.user_id
join AR.RA_CUST_TRX_TYPES_ALL ctt on ctt.CUST_TRX_TYPE_ID = cta.CUST_TRX_TYPE_ID and ctt.ORG_ID = cta.ORG_ID
--join INV.MTL_SYSTEM_ITEMS_B msi on ctla.INVENTORY_ITEM_ID = msi.INVENTORY_ITEM_ID
left join JTF.jtf_rs_salesreps jrs on cta.PRIMARY_SALESREP_ID = jrs.SALESREP_ID
left join JTF.JTF_RS_RESOURCE_EXTNS_tl rre on rre.RESOURCE_ID = jrs.RESOURCE_ID and rre.LANGUAGE = 'US'
left JOIN AR.ra_BATCH_SOURCES_ALL bs on cta.BATCH_SOURCE_ID = bs.BATCH_SOURCE_ID
-- Accounting String

left join ar.Ra_Cust_Trx_Line_Gl_Dist_All Trxgl on Trxgl.Customer_Trx_Line_Id=ctla.Customer_Trx_Line_Id
left join gl.GL_CODE_COMBINATIONS CC on Trxgl.Code_Combination_Id=cc.Code_Combination_Id
where 
CTLA.SALES_ORDER is null
and ctt.type = 'DM'

AND $X{IN,cas.attribute15,Sold_To_Group_Code_MSQ}
AND $X{IN,cab.attribute15,Bill_To_Group_Code_MSQ}
and $X{IN, ctt.CUST_TRX_TYPE_ID,Invoice_Type}


union all

--adjustments from the portal
select distinct
rre.RESOURCE_NAME as location

, cas.account_name as sold_to_acc_num
, cas.ACCOUNT_NUMBER as sold_to_acc_name
, cab.account_name as bill_to_acc_num
, cab.ACCOUNT_NUMBER as bill_to_acc_name

, COALESCE(cta.trx_number, cast(oha.order_number as varchar(20))) as invoice_number
, oha.attribute4 as vin
, COALESCE(cta.trx_date, oha.ordered_Date) as invoice_date
, COALESCE(decode(ctt.type, 'CM','Credit Memo', 'DM', 'Debit Memo', 'INV', 'Invoice'), otta.attribute1,otta.attribute2) as Invoice_type
, 'Credit_Memo' as Adjustment_Type
, COALESCE(ctla.line_type,ola.line_category_code) as type
, ola.CREATION_DATE as Adjustment_Date
--cr1099
--, ola.user_item_description as description
, COALESCE(CTLA.TRANSLATED_DESCRIPTION, ctla.description ,ola.user_item_description)as description
, COALESCE(ctla.description, msi.description)as item
, COALESCE(ctla.REASON_CODE,ola.line_category_code) as REASON_CODE
, COALESCE(ps.amount_due_original,case when ola.line_category_code = 'RETURN' then ola.ORDERED_QUANTITY * -1 * ola.UNIT_SELLING_PRICE else ola.ORDERED_QUANTITY * ola.UNIT_SELLING_PRICE end) as AMOUNT_DUE_ORIGINAL
, COALESCE(ctla.extended_amount,case when ola.line_category_code = 'RETURN' then ola.ORDERED_QUANTITY * -1 * ola.UNIT_SELLING_PRICE else ola.ORDERED_QUANTITY * ola.UNIT_SELLING_PRICE end) as Amount
, 'Approved' as Status
, f.user_name
, os.DESCRIPTION as source
, cc.segment1||'.'||cc.segment2||'.'||cc.segment3||'.'||cc.segment4||'.'||cc.segment5||'.'||cc.segment6||'.'||cc.segment7 as accounting_string
from ONT.OE_ORDER_HEADERS_ALL oha
join ONT.OE_ORDER_LINES_ALL ola on ola.HEADER_ID = oha.HEADER_ID
join applsys.fnd_user f on f.user_id = ola.CREATED_BY
join ONT.OE_ORDER_SOURCES os on os.ORDER_SOURCE_ID=ola.ORDER_SOURCE_ID
join ONT.OE_TRANSACTION_TYPES_ALL otta on otta.TRANSACTION_TYPE_ID = oha.ORDER_TYPE_ID
--cr1099
join INV.MTL_SYSTEM_ITEMS_B msi on ola.INVENTORY_ITEM_ID = msi.INVENTORY_ITEM_ID and msi.organization_id = 107 
left join AR.HZ_CUST_SITE_USES_ALL csua on csua.SITE_USE_ID = oha.invoice_TO_ORG_ID
left join AR.HZ_CUST_ACCT_SITES_ALL casa on casa.CUST_ACCT_SITE_ID = csua.CUST_ACCT_SITE_ID
-- bill to sold to
left join AR.HZ_CUST_ACCOUNTS cab on cab.CUST_ACCOUNT_ID = casa.CUST_ACCOUNT_ID
left join AR.HZ_CUST_ACCOUNTS cas on cas.CUST_ACCOUNT_ID = oha.SOLD_TO_ORG_ID

left join ont.oe_order_lines_all ola on ola.header_id = oha.header_id
left join JTF.jtf_rs_salesreps jrs on oha.SALESREP_ID = jrs.SALESREP_ID
left join JTF.JTF_RS_RESOURCE_EXTNS_tl rre on rre.RESOURCE_ID = jrs.RESOURCE_ID and rre.LANGUAGE = 'US'
left join ar.ra_customer_trx_lines_all ctla on ctla.sales_order = to_char(oha.order_number) and ctla.sales_order_line = ola.line_number
left join ar.ra_customer_trx_all cta on ctla.customer_trx_id = cta.customer_trx_id
left join ar.ar_payment_schedules_all ps on ps.customer_trx_id = cta.customer_trx_id
left join AR.RA_CUST_TRX_TYPES_ALL ctt on ctt.CUST_TRX_TYPE_ID = cta.CUST_TRX_TYPE_ID and ctt.ORG_ID = cta.ORG_ID
-- Accounting String

left join ar.Ra_Cust_Trx_Line_Gl_Dist_All Trxgl on Trxgl.Customer_Trx_Line_Id=ctla.Customer_Trx_Line_Id
left join gl.GL_CODE_COMBINATIONS CC on Trxgl.Code_Combination_Id=cc.Code_Combination_Id
where 
ola.ORDER_SOURCE_ID <>1001

AND $X{IN,cas.attribute15,Sold_To_Group_Code_MSQ}
AND $X{IN,cab.attribute15,Bill_To_Group_Code_MSQ}
and $X{IN, ctt.CUST_TRX_TYPE_ID,Invoice_Type}
)
where 1=1 

AND $X{IN,sold_to_acc_num,Sold_To_Account_Number_MSQ}
AND $X{IN,bill_to_acc_num,Bill_To_Account_Number_MSQ}

and $X{IN, LOCATION, Location} 
and trunc(ADJUSTMENT_DATE) >= nvl($P{Adjustment_Start_Date}, trunc(ADJUSTMENT_DATE))
and trunc(ADJUSTMENT_DATE) <= nvl($P{Adjustment_End_Date}, trunc(ADJUSTMENT_DATE))
 $P!{Invoice_Start_Date_sql}
 $P!{Invoice_End_Date_sql}

order by location, description, sold_to_acc_name, sold_to_acc_num, user_name]]>
		</queryString>
		<field name="LOCATION" class="java.lang.String"/>
		<field name="SOLD_TO_ACC_NUM" class="java.lang.String"/>
		<field name="SOLD_TO_ACC_NAME" class="java.lang.String"/>
